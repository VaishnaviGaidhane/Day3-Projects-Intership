pipeline{
    agent any
    stages{
        stage('Clone'){
            steps{
                git'https://github.com/VaishnaviGaidhane/Day3-Projects-Intership.git'
                }

        }
        stage('Build'){
            steps{
                sh 'echo "Building the project ..." '

            }
        }
        stage('Test'){
            steps{
                sh 'Python 3.13 -m unittest discover -s ./ -p "test_*.py"'
            }
        }

    }
}